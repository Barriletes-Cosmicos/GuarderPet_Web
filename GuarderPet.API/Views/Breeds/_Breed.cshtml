@model GuarderPet.API.Models.BreedViewModel

<div class="row">
    <div class="col-md-6">

        <div class="form-group">
            <label asp-for="BreedTittle" class="control-label"></label>
            <input asp-for="BreedTittle" class="form-control" />
            <span asp-validation-for="BreedTittle" class="text-danger"></span>
        </div>


        <div class="form-group">
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <select asp-for="Id" asp-items="Model.PetType" class="form-control"></select>
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
        </div>

    </div>
</div>